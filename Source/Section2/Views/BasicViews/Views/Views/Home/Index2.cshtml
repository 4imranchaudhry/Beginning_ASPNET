<html>
<head>
    <title> Views demo</title>
</head>
<body>
    @{
        for (int i = 0; i < 5; i++)
        {
            <li>@(i + 1)</li>
        }

        int j = 5;
        if (j < 10)
        {
            <text>@j is less than 10</text>
        }
        else
        {
            <text>j is greater than 10</text>
        }

    }
</body>
</html>